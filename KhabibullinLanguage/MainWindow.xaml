<Window x:Class="KhabibullinLanguage.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KhabibullinLanguage"
        mc:Ignorable="d"
        Title="Khabibullin Language" Height="450" Width="800" Icon="Res/school_logo.ico" FontFamily="Comic Sans MS">
    <Grid>
        <Image Source="Product/logo.png" HorizontalAlignment="Left"></Image>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="BtnBack" Width="175" Height="30" HorizontalAlignment="Right" VerticalAlignment="Top" Grid.Row="1" Click="BtnBack_Click" ></Button>
    </Grid>
</Window>
